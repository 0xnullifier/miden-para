{
  "name": "miden-para-react",
  "version": "0.10.4",
  "description": "React hook that wires Para accounts into a Miden client",
  "license": "MIT",
  "author": "Miden Labs",
  "repository": {
    "type": "git",
    "url": "https://github.com/0xMiden/miden-para",
    "directory": "packages/use-miden-para-react"
  },
  "homepage": "https://miden.xyz/",
  "bugs": {
    "url": "https://github.com/0xMiden/miden-para/issues"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "package.json"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "prepack": "npm run build",
    "publish": "npm run build && npm publish --access public"
  },
  "peerDependencies": {
    "@demox-labs/miden-sdk": "^0.12.5",
    "@getpara/react-sdk": "^2.0.0-alpha.73",
    "miden-para": "^0.10.4",
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@demox-labs/miden-sdk": "^0.12.5",
    "@getpara/react-sdk": "^2.0.0-alpha.73",
    "@types/react": "^19.2.5",
    "miden-para": "^0.10.4",
    "tsup": "^8.3.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "miden",
    "para",
    "react",
    "hook",
    "wallet"
  ]
}
